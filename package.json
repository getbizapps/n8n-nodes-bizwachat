{
  "name": "n8n-nodes-bizwachat",
  "version": "1.0.2",
  "description": "Bizwachat is a platform for automating WhatsApp messages, making it easy to start using the WhatsApp API. This node package allows you to use Bizwachat in n8n.",
  "keywords": [
    "n8n-community-node-package",
    "whatsapp",
    "messaging",
    "bizwachat",
    "automation"
  ],
  "license": "MIT",
  "homepage": "https://www.bizwachat.com/",
  "author": "Bizwachat <hello@bizwachat.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getbizapps/n8n-nodes-bizwachat.git"
  },
  "engines": {
    "node": ">=18.10"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "nodemon -w nodes -w credentials --ext ts --exec \"tsc && n8n\"",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes credentials package.json",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md",
    "index.js"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/BizwachatApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Bizwachat/Bizwachat.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "~5.45",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "~1.48.0",
    "prettier": "^2.7.1",
    "typescript": "~4.8.4"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "n8n-bizwachat-openapi-node": "^0.1.0"
  }
  "directories": {
    "test": "tests"
  },
  "bugs": {
    "url": "https://github.com/getbizapps/n8n-nodes-bizwachat/issues"
  }
}
